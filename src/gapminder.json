{
  "data": {
    "url": "https://raw.githubusercontent.com/vega/vega-datasets/master/data/gapminder.json"
  },
  "mark": "point",
  "encoding": {
    "color": {
      "field": "country"
    },
    "x": {
      "field": "fertility",
      "type": "quantitative"
    },
    "y": {
      "field": "life_expect",
      "type": "quantitative"
    },
    "time": {
      "field": "year",
      "interpolate": {
        "field": "country"
      }
    }
  }
}